    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "mongoose": "^7.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nodemon": "^2.0.22",
    "password-validator": "^5.3.0",
    "validator": "^13.9.0"
  }
